{
  "compilerOptions": {
    "incremental": true,
    "target": "es2022",
    "module": "node16",
    "lib": ["dom", "es2022"],
    "jsx": "react-jsx",
    "strict": true,
    "sourceMap": true,
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": ".erb/dll",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@/components/*": ["components/*"],
      "@/lib/*": ["lib/*"],
      "@/types/*": ["lib/types/*"],
      "@/services/*": ["lib/services/*"],
      "@/renderer/*": ["renderer/*"],
      "@/main/*": ["main/*"]
    }
  },
  "exclude": ["pocketbase", "release/build", "release/app/dist", ".erb/dll"]
}
